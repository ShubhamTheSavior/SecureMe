spring:
  application:
    name: secure-me-metadata
  profiles:
    active:
    - dev
  config:
    import:
    - optional:configserver:http://config:config@localhost:8888
server:
  port: 0
eureka:
  client:
    serviceUrl:
      defaultZone: ${EUREKA_URI:http://eureka:eureka@${eureka.ip}:${eureka.port}/eureka}
  instance:
    preferIpAddress: true
